# Production Environment Variables for TNCA IQ Platform Frontend
# This file will be used during Vercel deployment

# API Base URL - This will be set in Vercel environment variables
VITE_API_URL=https://your-render-backend-url.onrender.com/api

# App Configuration
VITE_APP_NAME=TNCA IQ Platform
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_WEBSOCKET=true
VITE_ENABLE_MAINTENANCE_MODE=false 